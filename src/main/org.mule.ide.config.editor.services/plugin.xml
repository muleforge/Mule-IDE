         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="mule_diagram"/>
               </and>
               <instanceof value="org.mule.ide.config.editor.services.navigator.CoreAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="org.mule.ide.config.editor.services.navigator.CoreAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="org.mule.ide.config.editor.services.navigatorSorter" 
               class="org.mule.ide.config.editor.services.navigator.CoreNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="mule_diagram"/>
                  </and>
                  <instanceof value="org.mule.ide.config.editor.services.navigator.CoreAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
