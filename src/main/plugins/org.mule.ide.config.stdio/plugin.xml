<plugin>
    
  <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
    <factory 
       uri = "http://www.mulesource.org/schema/mule/stdio/2.0" 
       class = "org.mule.ide.config.stdio.provider.StdioItemProviderAdapterFactory" 
       supportedTypes = 
         "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
          org.eclipse.emf.edit.provider.IStructuredItemContentProvider
          org.eclipse.emf.edit.provider.ITreeItemContentProvider
          org.eclipse.emf.edit.provider.IItemLabelProvider
          org.eclipse.emf.edit.provider.IItemPropertySource" />
  </extension>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package
       uri = "http://www.mulesource.org/schema/mule/stdio/2.0"
       class = "org.mule.ide.config.stdio.StdioPackage"
       genModel = "model/mule-stdio.genmodel" /> 
  </extension>

  <extension point="org.eclipse.emf.ecore.extension_parser">
    <parser
       type="stdio"
       class="org.mule.ide.config.stdio.util.StdioResourceFactoryImpl" />
  </extension>

  <extension
        point="org.eclipse.wst.xml.core.catalogContributions">
        <catalogContribution id="default">
            <uri
                name="http://www.mulesource.org/schema/mule/stdio/2.0"
                uri="model/mule-stdio.xsd" />
        </catalogContribution>
  </extension>

</plugin>
