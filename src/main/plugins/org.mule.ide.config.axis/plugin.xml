<plugin>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://www.mulesource.org/schema/mule/axis/2.0" 
       class = "org.mule.ide.config.axis.AxisPackage"
       genModel = "model/mule-axis.genmodel" /> 
  </extension>

  <extension point="org.eclipse.emf.ecore.extension_parser">
    <parser 
       type="axis" 
       class="org.mule.ide.config.axis.util.AxisResourceFactoryImpl" />
  </extension>

  <extension
        point="org.eclipse.wst.xml.core.catalogContributions">
        <catalogContribution id="default">
            <uri
                name="http://www.mulesource.org/schema/mule/axis/2.0"
                uri="model/mule-axis.xsd" />
        </catalogContribution>
  </extension>

</plugin>
