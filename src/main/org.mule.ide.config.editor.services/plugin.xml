            id="org.mule.ide.config.editor.services.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="org.mule.ide.config.editor.services.navigator.CoreNavigatorContentProvider" 
            labelProvider="org.mule.ide.config.editor.services.navigator.CoreNavigatorLabelProvider"
            icon="icons/obj16/CoreDiagramFile.gif"
