<plugin>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://www.springframework.org/schema/beans" 
       class = "org.mule.ide.config.spring.SpringPackage"
       genModel = "model/spring-beans-2.0.genmodel" /> 
  </extension>

  <extension point="org.eclipse.emf.ecore.extension_parser">
    <parser 
       type="spring" 
       class="org.mule.ide.config.spring.util.SpringResourceFactoryImpl" />
  </extension>

  <extension
        point="org.eclipse.wst.xml.core.catalogContributions">
        <catalogContribution id="default">
            <uri
                name="http://www.springframework.org/schema/beans"
                uri="model/spring-beans-2.0.xsd" />
        </catalogContribution>
  </extension>

</plugin>
